                    
@model OrcamentoData.Orcamentos

@{
    ViewBag.Title = "EditOrcamentos";
}

<h2>EditOrcamentos</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Orcamentos</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.IdOrcamento)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.IdOrcamento)
            @Html.ValidationMessageFor(model => model.IdOrcamento)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "IndexProdutos")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
